{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug ctsp_scheduler (build)",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/bin/ctsp_scheduler",
            "args": [
                "ctsp2",
                "${workspaceFolder}/input//home/riera/ConsistentTSP_code_backup/Instances/SubramanyamGounaris/burma14_p3_f50_lH.contsp.contsp",
                "${workspaceFolder}/input//home/riera/ConsistentTSP_code_backup/Instances/SubramanyamGounaris/burma14_p3_f50_lH.contsp.sol",
                "${workspaceFolder}/output//home/riera/ConsistentTSP_code_backup/Instances/SubramanyamGounaris/burma14_p3_f50_lH.contsp.sched.json"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "Build ctsp_scheduler (build)",
            "miDebuggerPath": "/usr/bin/gdb",
            "logging": {
                "moduleLoad": false,
                "trace": false,
                "engineLogging": false,
                "programOutput": true,
                "exceptions": false
            }
        },
